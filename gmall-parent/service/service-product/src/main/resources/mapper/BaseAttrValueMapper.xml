<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.king.gmall.product.mapper.BaseAttrValueMapper">
    <insert id="batchInsert">
        INSERT INTO
            base_attr_value(value_name, attr_id)
        VALUES
            <foreach collection="list" item="baseAttrValue" separator=",">
                (
                 #{baseAttrValue.valueName},#{baseAttrValue.attrId}
                )
            </foreach>

    </insert>
</mapper>